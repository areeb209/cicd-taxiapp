pipeline {
    agent {
        node {
            label 'maven'
        }
    }
environment {
    PATH = "/usr/share/maven-3.6.3/bin:$PATH"
    
}
   stages {
        stage("build"){
            steps {
                 echo "----------- build started ----------"
                sh 'mvn package'
                 echo "----------- build complted ----------"
            }
        }
}
}
